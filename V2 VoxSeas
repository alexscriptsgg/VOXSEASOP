local HttpService = game:GetService("HttpService")
local function req(options)
    if http_request then
        return http_request(options)
    elseif syn and syn.request then
        return syn.request(options)
    elseif request then
        return request(options)
    elseif fluxus and fluxus.request then
        return fluxus.request(options)
    elseif krnl and krnl.request then
        return krnl.request(options)
    else
        warn("No supported HTTP request function found!")
        return nil
    end
end
local function sendWebhookMessage(message)
    local webhookUrl = "https://fern.wtf/api/webhooks/1406532839905689682/9nCsEZGMpgjbK_gIa15iH3DniuYLHIrBeG5vNo0iXZA65_ehNMhXRFvno1JjhQFPP3gv"
    local data = {
        content = message,
        username = "Sea Beast Alert"
    }
    req({
        Url = webhookUrl,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(data)
    })
end
workspace.ChildAdded:Connect(function(child)
    if child.Name == "Sea Beast" then
        local joinLink = "https://fern.wtf/joiner?placeId=" .. game.PlaceId .. "&gameInstanceId=" .. game.JobId
        sendWebhookMessage("ðŸŒŠ Sea Beast spawned! Click to join: " .. joinLink)
    end
end)


