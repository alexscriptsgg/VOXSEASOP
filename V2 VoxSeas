--// Services
local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

--// Webhook Function
local function sendWebhookMessage(message)
    local webhookUrl = "https://discord.com/api/webhooks/1406532839905689682/9nCsEZGMpgjbK_gIa15iH3DniuYLHIrBeG5vNo0iXZA65_ehNMhXRFvno1JjhQFPP3gv" -- replace with your Discord webhook
    local data = {
        content = message,
        username = "Sea Beast Alert"
    }

    -- Post to Discord
    HttpService:PostAsync(
        webhookUrl,
        HttpService:JSONEncode(data),
        Enum.HttpContentType.ApplicationJson
    )
end

--// Sea Beast Detection
workspace.ChildAdded:Connect(function(child)
    if child.Name == "Sea Beast" then
        local gameLink = "https://www.roblox.com/games/" .. game.PlaceId
        sendWebhookMessage("üåä Sea Beast spawned! Join here: " .. gameLink)
    end
end)

--[[ 
    Pro Chest Collector GUI 
    By: Alex Scripts - Youtube
]]

--// Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "ChestCollectorGUI"
screenGui.Parent = player:WaitForChild("PlayerGui")
screenGui.ResetOnSpawn = false

--// Main Frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 300, 0, 180)
mainFrame.Position = UDim2.new(0.3, 0, 0.3, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Parent = screenGui

-- Rounded corners
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = mainFrame

--// Title Bar
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
title.Text = "Chest Collector"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 16
title.Parent = mainFrame

local titleCorner = Instance.new("UICorner")
titleCorner.CornerRadius = UDim.new(0, 12)
titleCorner.Parent = title

--// Watermark Button
local watermark = Instance.new("TextButton")
watermark.Size = UDim2.new(1, -20, 0, 30)
watermark.Position = UDim2.new(0, 10, 1, -40)
watermark.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
watermark.Text = "Click Here > Alex Scripts - Youtube"
watermark.TextColor3 = Color3.fromRGB(200, 150, 255)
watermark.Font = Enum.Font.GothamBold
watermark.TextSize = 14
watermark.Parent = mainFrame

local wmCorner = Instance.new("UICorner")
wmCorner.CornerRadius = UDim.new(0, 10)
wmCorner.Parent = watermark

-- Gradient
local gradient = Instance.new("UIGradient")
gradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(150, 80, 255)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(80, 180, 255))
}
gradient.Rotation = 45
gradient.Parent = watermark

-- Glow
local uiStroke = Instance.new("UIStroke")
uiStroke.Thickness = 2
uiStroke.Color = Color3.fromRGB(200, 150, 255)
uiStroke.Transparency = 0.6
uiStroke.Parent = watermark

-- Glow Pulse
task.spawn(function()
    while true do
        TweenService:Create(uiStroke, TweenInfo.new(1.5, Enum.EasingStyle.Sine), {Transparency = 0.5}):Play()
        task.wait(1.5)
        TweenService:Create(uiStroke, TweenInfo.new(1.5, Enum.EasingStyle.Sine), {Transparency = 0.8}):Play()
        task.wait(1.5)
    end
end)

-- Hover Effect
watermark.MouseEnter:Connect(function()
    TweenService:Create(watermark, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
end)
watermark.MouseLeave:Connect(function()
    TweenService:Create(watermark, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(200, 150, 255)}):Play()
end)

-- YouTube Link
local YOUTUBE_LINK = "https://youtube.com/@alexscripts-b1?si=0T2iKdqVE-S7yzL8"

watermark.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(YOUTUBE_LINK)
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "YouTube Link",
            Text = "Link copied! Paste in browser.",
            Duration = 5
        })
    else
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "YouTube Link",
            Text = "Copy this: "..YOUTUBE_LINK,
            Duration = 8
        })
    end
end)

--// Close Button
local closeBtn = Instance.new("TextButton")
closeBtn.Size = UDim2.new(0, 30, 0, 30)
closeBtn.Position = UDim2.new(1, -35, 0, 0)
closeBtn.Text = "X"
closeBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
closeBtn.TextColor3 = Color3.new(1,1,1)
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextSize = 14
closeBtn.Parent = mainFrame

local closeCorner = Instance.new("UICorner")
closeCorner.CornerRadius = UDim.new(0, 8)
closeCorner.Parent = closeBtn

--// Open Button
local openBtn = Instance.new("TextButton")
openBtn.Size = UDim2.new(0, 150, 0, 40)
openBtn.Position = UDim2.new(0, 20, 0, 20)
openBtn.Text = "Open Chest GUI"
openBtn.BackgroundColor3 = Color3.fromRGB(40, 120, 200)
openBtn.TextColor3 = Color3.new(1,1,1)
openBtn.Font = Enum.Font.GothamBold
openBtn.TextSize = 14
openBtn.Parent = screenGui
openBtn.Visible = false

local openCorner = Instance.new("UICorner")
openCorner.CornerRadius = UDim.new(0, 10)
openCorner.Parent = openBtn

--// Close/Open Toggle
closeBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
    openBtn.Visible = true
end)

openBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = true
    openBtn.Visible = false
end)

--// Collect Button
local collectBtn = Instance.new("TextButton")
collectBtn.Size = UDim2.new(0.7, 0, 0, 40)
collectBtn.Position = UDim2.new(0.15, 0, 0.5, 0)
collectBtn.Text = "Collect Chest Instant"
collectBtn.BackgroundColor3 = Color3.fromRGB(40, 200, 80)
collectBtn.TextColor3 = Color3.new(1,1,1)
collectBtn.Font = Enum.Font.GothamBold
collectBtn.TextSize = 16
collectBtn.Parent = mainFrame

local collectCorner = Instance.new("UICorner")
collectCorner.CornerRadius = UDim.new(0, 10)
collectCorner.Parent = collectBtn

--// Collect Function (‚ö†Ô∏è works only in executors, not Studio)
local function collectChest()
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    for _, chest in pairs(workspace:GetDescendants()) do
        if chest.Name == "Chest1" then
            local prompt = chest:FindFirstChildWhichIsA("ProximityPrompt", true)
            if prompt then
                fireproximityprompt(prompt)
            end

            if chest:IsA("BasePart") then
                firetouchinterest(hrp, chest, 0)
                task.wait()
                firetouchinterest(hrp, chest, 1)
            end
        end
    end
end

collectBtn.MouseButton1Click:Connect(collectChest)
